{
  "expo": {
    "name": "ناسبني",
    "slug": "nasibni",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/logos/Logo1.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "nasibni",
    "splash": {
      "image": "./src/assets/logos/Group1.png",
      "resizeMode": "contain",
      "backgroundColor": "#4F2396"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nasibni.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logos/Logo1.png",
        "backgroundColor": "#4F2396"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nasibni.app"
    },
    "web": {
      "favicon": "./src/assets/logos/Logo1.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser"
    ]
  }
}
